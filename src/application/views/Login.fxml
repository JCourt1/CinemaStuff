      </Button>
      <TextField fx:id="txtUsername" layoutX="34.0" layoutY="79.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="34.0" layoutY="130.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblStatus" layoutX="34.0" layoutY="36.0" prefHeight="17.0" prefWidth="167.0" text="Status" textFill="#2da2c9">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="regButton" layoutX="57.0" layoutY="239.0" mnemonicParsing="false" onAction="#GoToReg" prefHeight="27.0" prefWidth="179.0" text="Register" />
