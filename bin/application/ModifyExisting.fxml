   <children>
      <TableView fx:id="filmTable" layoutX="38.0" layoutY="44.0" prefHeight="313.0" prefWidth="176.0">
        <columns>
          <TableColumn fx:id="filmNameColumn" prefWidth="173.0" text="Films" />
        </columns>
      </TableView>
      <Button fx:id="deleteBtn" layoutX="355.0" layoutY="112.0" mnemonicParsing="false" onAction="#deleteFilms" text="Delete film" />
      <ImageView fx:id="imgView" fitHeight="181.0" fitWidth="200.0" layoutX="286.0" layoutY="162.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
